{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Account",
    "description": "The Account",
    "required": [
        "name",
        "balance",
        "user_id",
        "account_type"
  ],
  "additionalProperties": false,
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The database ID of the Account Type."
      },
      "name": {
          "$id": "#/properties/name",
          "type": "string",
          "title": "The name of the Account"
      },
      "balance": {
        "$id": "#/properties/balance",
        "type": "number",
        "title": "The balance of the Account"
    },
      "user_id": {
        "$id": "#/properties/user_id",
        "type": "number",
        "title": "The id of the User Type"
    },
      "account_type_id": {
        "$id": "#/properties/account_type_id",
        "type": "number",
        "title": "The id of the Account Type"
    }
  }
  }