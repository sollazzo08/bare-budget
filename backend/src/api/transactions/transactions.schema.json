{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Transaction",
  "description": "The Transaction",
  "required": [
    "amount",
    "date",
    "note",
    "category_id",
    "transaction_type_id",
    "account_id",
    "user_id"
],
"additionalProperties": false,
"properties": {
    "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "The database ID of the Transaction."
    },
    "amount": {
        "$id": "#/properties/amount",
        "minimum": 0,
        "type": "number",
        "title": "The amount of the transaction"
    },
    "date": {
        "$id": "#/properties/date",
        "type": "string",
        "title": "The date of transaction"
    },
    "note": {
        "$id": "#/properties/note",
        "type": "string",
        "title": "The note of the Transaction"
    },
    "category_id": {
        "$id": "#/properties/category_id",
        "type": "number",
        "title": "The id of the Category Type"
    },
    "transaction_type_id": {
        "$id": "#/properties/transaction_type_id",
        "type": "number",
        "title": "The id of the Transaction Type"
    },
    "account_id": {
        "$id": "#/properties/account_id",
        "type": "number",
        "title": "The id of the Account Type"
    },
    "user_id": {
        "$id": "#/properties/user_id",
        "type": "number",
        "title": "The id of the User"
    }     
}
}