{
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Transaction Tag",
    "description": "The Transaction Tag",
    "required": [
        "name",
        "transaction_id",
        "user_id"
  ],
  "additionalProperties": false,
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The database ID of the Transaction Tag."
      },
      "transaction_id": {
          "$id": "#/properties/transaction_id",
          "type": "integer",
          "title": "The id of the Transaction"
      },
      "user_id": {
        "$id": "#/properties/user_id",
        "type": "integer",
        "title": "The id of the User"
    }       
   }
  }