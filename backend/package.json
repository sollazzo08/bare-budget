    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/yup": "^0.32.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^4.2.5",
    "nodemon": "^3.1.11",
    "prettier": "^2.8.8",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
