{
  "name": "backend",
  "version": "1.0.0",
  "description": "Budget App to help track my income and spending",
  "scripts": {
    "start": "node ./bin/www",
    "build": "tsc --project .",
    "dev": "nodemon src/index.ts",
    "migrate": "npx prisma migrate dev --name init",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset --force --skip-seed",
    "generate": "npx prisma generate",
    "lint": "eslint . --ext .ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
